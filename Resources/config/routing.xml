<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="theapi_cctvblindfold" pattern="/">
        <default key="_controller">TheapiCctvBlindfoldBundle:Default:index</default>
    </route>
    
    <route id="theapi_cctvblindfold_toggle" pattern="/toggle">
        <default key="_controller">TheapiCctvBlindfoldBundle:Default:toggle</default>
    </route>
    
</routes>
